{
	"compilerOptions": {
		"noImplicitAny": true,
		"strictNullChecks": true,
		"noFallthroughCasesInSwitch": true,
		"noUnusedParameters": true,
		"noImplicitReturns": true,
		"module": "commonjs",
		"target": "es6",
		"outDir": "out",
		"lib": [
			"es6"
		],
		"sourceMap": true,
		"rootDir": ".",
		"strict": true   /* enable all strict type-checking options */
	},
	"exclude": [
		"node_modules",
		".vscode-test"
	],
}
